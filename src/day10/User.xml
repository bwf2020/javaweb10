<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="day10.User">

<resultMap type="" id="">


<association property=""></association>

</resultMap>

<select id="find1" resultType="User">

	SELECT a.*,b.*
	FROM USER AS a
	LEFT JOIN card AS b
	ON a.id=b.uid
	WHERE a.name=#{name}

</select>



</mapper>